{
  "name": "editorserver",
  "version": "1.0.0",
  "description": "my online editor",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./dist/server/server.ts",
    "start": "ts-node ./server/server.ts",
    "prod": "npm run build && npm run start"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "node_modules"
    ],
    "watch": [
      "*"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "Manjesh K M",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-winston": "^4.0.0",
    "@types/lodash": "^4.14.144",
    "@types/morgan": "^1.7.37",
    "@types/request": "^2.48.3",
    "@types/winston": "^2.4.4",
    "app-root-path": "^3.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.0.1",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
